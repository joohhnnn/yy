package com.txznet.webchat.plugin

public class WxPluginInfo {
    // 插件名, 插件输出格式默认为${pluginName}#${pluginVersion}.jar
    String pluginName
    // 插件版本号 "x.x.x"
    String pluginVersionName

    // debug模式的buildType
    String debugBuildType = "debug"
    // release模式的buildType
    String releaseBuildType

    // debug模式的输出路径
    String debugOutPath
    // release模式的输出路径
    String releaseOutPath

    // 插件在预置模式下的包名, 用作插件发布模式替换包名依据
    String presetPackageName = "com.txznet.webchat.plugin.preset"
    // debug模式的包名, 用作插件发布模式替换包名依据
    String debugPackageName = "com.txznet.webchat.plugin"
    // release模式的包名, 用作插件发布模式替换包名依据
    String releasePackageName = "com.txznet.webchat.plugin"
}
