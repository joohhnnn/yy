<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TitleBar">
        <attr name="leftIcon" format="reference|color" />
        <attr name="rightText" format="reference|string" />
        <attr name="titleText" format="reference|string" />
        <attr name="rightImg" format="reference|string" />
    </declare-styleable>
    <declare-styleable name="GifTextureView">
        <attr format="reference|string" name="gifSource"/>
        <attr format="boolean" name="isOpaque"/>
    </declare-styleable>
    <declare-styleable name="GifView">
        <attr format="boolean" name="freezesAnimation"/>
        <attr format="integer" name="loopCount"/>
    </declare-styleable>
</resources>
