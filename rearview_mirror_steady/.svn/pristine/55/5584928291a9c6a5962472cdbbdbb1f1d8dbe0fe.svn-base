<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="base_page_height" format="dimension" />
    <attr name="rec_page_large_item_width" format="dimension" />
    <attr name="rec_page_large_item_height" format="dimension" />
    <attr name="rec_page_item_size" format="dimension" />
    <attr name="base_page_item_size" format="dimension" />
    <attr name="page_item_offset_1_1" format="dimension" />
    <attr name="page_item_offset_1_1_1" format="dimension" />
    <attr name="choice_page_width" format="dimension" />
    <attr name="player_bar_cover_size" format="dimension" />
    <attr name="player_bar_logo_size" format="dimension" />
    <attr name="player_bar_control_size" format="dimension" />
    <attr name="radio_listen_list_width" format="dimension" />
    <attr name="radio_listen_list_max_width" format="dimension" />

    <declare-styleable name="ResourceButton">
        <attr name="resbtn_padding_vertical" format="dimension" />
        <attr name="resbtn_padding_horizontal" format="dimension" />
        <attr name="resbtn_padding_icon" format="dimension" />
        <attr name="resbtn_size_icon" format="dimension" />
        <attr name="resbtn_src_icon_normal" format="reference" />
        <attr name="resbtn_src_icon_pressed" format="reference" />
        <attr name="resbtn_size_text" format="dimension" />
        <attr name="resbtn_color_text_normal" format="color" />
        <attr name="resbtn_color_text_pressed" format="color" />
        <attr name="resbtn_str_text" format="string" />
    </declare-styleable>
    <declare-styleable name="LyricView">
        <attr name="hint" format="string" />
        <attr name="hintColor" format="color" />
        <attr name="textColor" format="color" />
        <attr name="highlightColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="maxLength" format="dimension" />
        <attr name="fadeInFadeOut" format="boolean" />
        <attr name="lineSpace" format="dimension" />
        <attr name="textPlaySize" format="dimension" />
        <attr name="textAlign">
            <enum name="left" value="0" />
            <enum name="center" value="1" />
            <enum name="right" value="2" />
        </attr>
    </declare-styleable>

    <attr name="cornerRadius" format="dimension" />

    <declare-styleable name="LoadingView">
        <attr name="loadingView" format="reference" />
        <attr name="errorView" format="reference" />
        <attr name="retryView" format="reference" />
        <attr name="emptyView" format="reference" />
        <attr name="contentsView" format="reference" />
        <attr name="animation" format="reference" />
    </declare-styleable>
    <declare-styleable name="TextImageView">
        <attr name="drawableLeftWidth" format="dimension" />
        <attr name="drawableLeftHeight" format="dimension" />
        <attr name="drawableTopWidth" format="dimension" />
        <attr name="drawableTopHeight" format="dimension" />
        <attr name="drawableRightWidth" format="dimension" />
        <attr name="drawableRightHeight" format="dimension" />
        <attr name="drawableBottomWidth" format="dimension" />
        <attr name="drawableBottomHeight" format="dimension" />
        <attr name="clickEffect" format="boolean" />
        <!--链接：https://www.jianshu.com/p/09a5399dc26b-->
    </declare-styleable>
</resources>