<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/layoutDialog"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/win_layout_card_margin_hor_screen"
    android:paddingTop="@dimen/win_layout_card_margin_ver_screen"
    android:paddingRight="@dimen/win_layout_card_margin_hor_screen"
    android:paddingBottom="@dimen/win_layout_card_margin_ver_screen"
    android:visibility="visible">

    <RelativeLayout
        android:id="@+id/layoutDialogMask"
        android:layout_width="@dimen/win_layout_card_width"
        android:layout_height="match_parent"
        android:background="@drawable/xml_dialog_bg_mask">

        <FrameLayout
            android:id="@+id/layoutDialogContentWrap"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:orientation="vertical">

<!--            <include layout="@layout/dialog_confirm" />-->

        </FrameLayout>

    </RelativeLayout>

</FrameLayout>