package com.txznet.webchat.model;

/**
 * 数据上报模型
 * Created by ASUS User on 2016/4/19.
 */
public class ReportMessage {

    /*
    * 上报消息类型定义
    * 100 微信登陆
    * 101 微信注销
    *
    * 点击主界面按钮：
    * 200 最近会话
    * 201 新手指南
    * 202 自动播报开启
    * 203 自动播报关闭
    * 204 关注设备
    * 205 退出按钮
    * 聊天界面交互：
    * 210 点击回复微信
    * 211 进入聊天界面并回复消息
    * 212 进入聊天界面没有回复消息
    * 帮助页面交互：
    * 220 打开帮助页面条目
    *
    * 语音相关：
    * 300 发微信
    * 301 回复微信
    * 302 屏蔽消息
    * 303 打开自动播报
    * 304 关闭自动播报
    * 305 查看历史消息
    * 306 继续发送
    * 307 录音完毕
    * 308 取消发送
    * 309 退出微信
    * 3010 登录微信
    *
    * */
    public static final int REPORT_WECHAT_LOGIN = 100;
    public static final int REPORT_WECHAT_LOGOUT = 101;
    public static final int REPORT_UI_MAINPAGE_SESSION = 200;
    public static final int REPORT_UI_MAINPAGE_HELP = 201;
    public static final int REPORT_UI_MAINPAGE_NOTIFY_ENABLE = 202;
    public static final int REPORT_UI_MAINPAGE_NOTIFY_DISABLE = 203;
    public static final int REPORT_UI_MAINPAGE_CARE = 204;
    public static final int REPORT_UI_MAINPAGE_EXIT = 205;
    public static final int REPORT_UI_CHAT_SEND = 210;
    public static final int REPORT_UI_CHAT_WITH_SEND = 211;
    public static final int REPORT_UI_CHAT_WITHOUT_SEND = 212;
    public static final int REPORT_UI_HELP_OPEN = 220;
    public static final int REPORT_VOICE_SEND = 300;
    public static final int REPORT_VOICE_REPLY = 301;
    public static final int REPORT_VOICE_SHIELD = 302;
    public static final int REPORT_VOICE_NOTIFY_ENABLE = 303;
    public static final int REPORT_VOICE_NOTIFY_DISABLE = 304;
    public static final int REPORT_VOICE_OPEN_HISTORY = 305;
    public static final int REPORT_VOICE_CONTINUE_SEND = 306;
    public static final int REPORT_VOICE_RECORD_COMPLETE = 307;
    public static final int REPORT_VOICE_RECORD_CANCEL = 308;
    public static final int REPORT_VOICE_LOGOUT_WECHAT = 309;
    public static final int REPORT_VOICE_LOGIN_WECHAT = 3010;
}
