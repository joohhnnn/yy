<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape>
            <solid android:color="#1cffffff" />

            <corners android:radius="@dimen/x4" />

            <size
                android:width="@dimen/x650"
                android:height="@dimen/y360" />
        </shape>
    </item>

    <item android:top="@dimen/y2">
        <shape>
            <gradient
                android:angle="-180"
                android:endColor="#3e4351"
                android:startColor="#2c313e" />

            <corners android:radius="@dimen/x4" />

            <size
                android:width="@dimen/x650"
                android:height="@dimen/y360" />
        </shape>
    </item>
</layer-list>