        <!-- 支持ApkLoader框架 -->
        <meta-data android:name="TXZApkLoader" android:value="true"/>
        
        <!-- ====================框架服务定义==================== -->
      <!-- 同行者框架核心服务 -->
        <service
            android:name="com.txznet.music.service.MyService"
            android:exported="true" >
            <intent-filter android:priority="2147483647" >
                <action android:name="com.txznet.music.service.TXZService" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </service>
        <!-- 同行者服务唤醒广播 -->
        <receiver android:name="com.txznet.comm.base.StartReciver" >
            <intent-filter android:priority="2147483647">
                <action android:name="com.txznet.music.startTXZService" />
            </intent-filter>
        </receiver>
        <!-- 同行者URI API预留接口 -->
        <receiver android:name="com.txznet.comm.base.UriInvokeReciver" >
            <intent-filter android:priority="2147483647">
                <action android:name="com.txznet.music.invoke" />
                <data android:scheme="txznet" android:host="com.txznet.music" />
            </intent-filter>
        </receiver>
        
        <!-- ====================框架预留服务==================== -->
