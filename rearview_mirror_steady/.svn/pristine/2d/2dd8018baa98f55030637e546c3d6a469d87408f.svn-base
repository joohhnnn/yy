<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <corners
                android:radius="4dp" />
            <solid
                android:color="#3FFFFFFF" />
            <stroke
                android:width="1px"
                android:color="#4dffffff" />
        </shape>
    </item>

    <item>
        <shape>
            <corners
                android:radius="4dp" />
            <solid
                android:color="#0dffffff" />
            <stroke
                android:width="1px"
                android:color="#4dffffff" />
        </shape>
    </item>
</selector>
