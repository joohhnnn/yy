package com.txznet.music.ui.helper.entity;

import android.view.View;

/**
 * @author QBW
 * @createtime 2016/04/05 11:26
 */


public class GroupChild extends BaseEntity {

    public String LeftText;
    public String mSpeakText;
    //是否需要展示右边的数据
    public boolean isNeedShowRight;


    public GroupChild(String text, String speakText, boolean needShowRight, View.OnClickListener listener) {
        super(text);
        LeftText = text;
        mSpeakText = speakText;
        isNeedShowRight = needShowRight;
        mOnClickListener = listener;
    }
}
